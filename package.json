{
  "name": "ra-hasura-typescript-boilerplate",
  "version": "1.0.0",
  "description": "react admin hasura typescript boilerplate",
  "default": "dist/index.html",
  "sideEffects": false,
  "scripts": {
    "dev": "webpack serve --mode development --env development --config ./config/webpack.config.dev.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run lint && webpack --config ./config/webpack.config.prod.js",
    "build:prod": "npm run clean && npm run lint && webpack --env.NODE_ENV=production --config ./config/webpack.config.prod.js",
    "test": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint --ext .js,.ts src"
  },
  "keywords": [
    "react",
    "admin",
    "hasura",
    "typescript",
    "boilerplate"
  ],
  "author": "Toan Nguyen",
  "license": "MIT",
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/node": "^14.14.12",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cypress": "^6.1.0",
    "cypress-react-unit-test": "^4.17.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "next",
    "husky": "^4.3.5",
    "source-map-loader": "^1.1.3",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@apollo/react-hooks": "^4.0.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "graphql": "^15.4.0",
    "ra-data-hasura-graphql": "^0.1.13",
    "react": "^17.0.1",
    "react-admin": "^3.10.4",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "husky": {
    "hooks": {
      "pre-push": "sh ./scripts/build.sh"
    }
  }
}
